<view class="item animated fadeInLeft fast"><view class="header"><view class="header-left"><image class="portrait" src="{{tempList.userPic}}"></image><text class="name">{{tempList.userName}}</text></view><view class="header-right"><block><block wx:if="{{!tempList.isGuanZhu}}"><view data-event-opts="{{[['tap',[['attention',['$event']]]]]}}" class="attention icon iconfont icon-zengjia" bindtap="__e">关注</view></block></block><view class="delete icon iconfont icon-guanbi"></view></view></view><view class="title">{{''+tempList.title+''}}</view><block wx:if="{{tempList.type=='img'}}"><view data-event-opts="{{[['tap',[['goDetails',['$event']]]]]}}" class="content" bindtap="__e"><image class="img" mode="widthFix" src="{{tempList.content}}"></image></view></block><block wx:if="{{tempList.type=='video'}}"><view data-event-opts="{{[['tap',[['goVideo',['$event']]]]]}}" class="content" bindtap="__e"><image class="img" mode="widthFix" src="{{tempList.content}}"></image><view><view class="pause icon iconfont icon-bofang"></view><view class="playnum">{{''+tempList.playNum+"万次播放"+tempList.timeNum}}</view></view></view></block><view class="bottom"><view class="left"><view data-event-opts="{{[['tap',[['caozuo',['ding']]]]]}}" class="praise" bindtap="__e"><block wx:if="{{tempList.infoNum.status==1}}"><block><image class="iconImg" src="../../static/images/praise_active.png"></image></block></block><block wx:else><block><image class="iconImg" src="../../static/images/praise.png"></image></block></block><text class="count">{{''+tempList.infoNum.praiseNum+''}}</text></view><view data-event-opts="{{[['tap',[['caozuo',['cai']]]]]}}" class="trample" bindtap="__e"><block wx:if="{{tempList.infoNum.status==2}}"><block><image class="iconImg" src="../../static/images/trample_active.png"></image></block></block><block wx:else><block><image class="iconImg" src="../../static/images/trample.png"></image></block></block><text class="count">{{''+tempList.infoNum.trampleNum+''}}</text></view></view><view class="right"><view class="comment"><image class="iconImg" src="../../static/images/comment.png"></image><view class="count">{{''+tempList.commentNum}}</view></view><view class="transpond"><image class="iconImg" src="../../static/images/transpond.png"></image><view class="count">{{tempList.transpondNum}}</view></view></view></view></view>